{
  "name": "@fabalous/runtime-web",
  "version": "0.2.94",
  "scripts": {
    "postversion": "npm run build && git push && git push --tags && npm publish",
    "build": "gulp clean && tsc && gulp copy_src_to_lib && gulp remove_src_folder && gulp remove_node_modules_folder",
    "install": "shx cp -Rf ./lib/* ./ && shx rm -r ./lib",
    "test": "jest --no-cache",
    "tdd": "jest --watch",
    "coverage": "jest --no-cache --coverage",
    "doc": "gulp typedoc"
  },
  "author": "JÃ¶rg Wasmeier",
  "license": "ISC",
  "dependencies": {
    "@fabalous/core": "^0.5.31",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "compression-webpack-plugin": "^1.1.7",
    "file-loader": "^1.1.6",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "happypack": "^4.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "offline-plugin": "^4.9.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "shx": "^0.2.2",
    "ts-loader": "^3.5.0",
    "tslib": "^1.9.0",
    "typescript": "^2.7.2",
    "typestyle": "^1.7.1",
    "uglify-js": "^3.3.11",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^0.6.1",
    "gulp-typedoc": "^2.2.0",
    "gulp-util": "^3.0.8",
    "jest": "^22.3.0",
    "jest-cli": "^22.3.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.10.0",
    "uglifyjs-webpack-plugin": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Faba-network/fabalous-runtime-web.git"
  },
  "bugs": {
    "url": "https://github.com/Faba-network/fabalous-runtime-web/issues"
  },
  "homepage": "https://github.com/Faba-network/fabalous-runtime-web",
  "description": "",
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.*\\Spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
