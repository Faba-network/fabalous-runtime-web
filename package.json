{
  "name": "@fabalous/runtime-web",
  "version": "0.2.34",
  "scripts": {
    "postversion": "npm run build && git push && git push --tags && npm publish",
    "build": "gulp clean && tsc && gulp copy_src_to_lib && gulp remove_src_folder && gulp remove_node_modules_folder",
    "install": "shx cp -Rf ./lib/* ./ && shx rm -r ./lib",
    "test": "jest --no-cache",
    "tdd": "jest --watch",
    "coverage": "jest --no-cache --coverage",
    "doc": "gulp typedoc"
  },
  "author": "JÃ¶rg Wasmeier",
  "license": "ISC",
  "dependencies": {
    "@fabalous/core": "*",
    "@types/react": "*",
    "@types/react-addons-css-transition-group": "*",
    "@types/react-addons-perf": "*",
    "@types/react-dom": "*",
    "awesome-typescript-loader": "3.1.3",
    "babel-preset-react-optimize": "^1.0.1",
    "compression-webpack-plugin": "^0.4.0",
    "file-loader": "^0.11.1",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.28.0",
    "offline-plugin": "^4.6.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "tslib": "^1.6.0",
    "typescript": "^2.2.2",
    "typestyle": "^1.3.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "shx": "^0.2.2",
    "babel-preset-babili": "0.0.12",
    "babili": "0.0.12",
    "babili-webpack-plugin": "0.0.11"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-replace": "^0.5.4",
    "gulp-util": "^3.0.8",
    "gulp-typedoc": "^2.0.2",
    "@types/jest": "^19.2.2",
    "jest": "^20.0.3",
    "jest-cli": "^20.0.3",
    "ts-jest": "^20.0.4",
    "tslint": "^5.0.0",
    "typedoc": "^0.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Faba-network/fabalous-runtime-web.git"
  },
  "bugs": {
    "url": "https://github.com/Faba-network/fabalous-runtime-web/issues"
  },
  "homepage": "https://github.com/Faba-network/fabalous-runtime-web",
  "description": "",
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.*\\Spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
