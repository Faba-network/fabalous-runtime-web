{
  "name": "@fabalous/runtime-web",
  "version": "1.0.3",
  "scripts": {
    "postversion": "npm run build && git push && git push --tags && npm publish",
    "build": "gulp clean && tsc && gulp copy_src_to_lib && gulp remove_src_folder && gulp remove_node_modules_folder",
    "install": "shx cp -Rf ./lib/* ./ && shx rm -r ./lib",
    "test": "jest --no-cache",
    "tdd": "jest --watch",
    "coverage": "jest --no-cache --coverage",
    "doc": "gulp typedoc"
  },
  "author": "JÃ¶rg Wasmeier",
  "license": "ISC",
  "dependencies": {
    "@fabalous/core": "^1.0.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "compression-webpack-plugin": "^1.1.11",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "happypack": "^5.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "offline-plugin": "^5.0.5",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "shx": "^0.3.2",
    "ts-loader": "^4.4.2",
    "tslib": "^1.9.3",
    "typescript": "^2.9.2",
    "typestyle": "^2.0.1",
    "uglify-js": "^3.4.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-dev-server": "^3.1.4"
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-typedoc": "^2.2.0",
    "gulp-util": "^3.0.8",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.1",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "uglifyjs-webpack-plugin": "^1.2.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Faba-network/fabalous-runtime-web.git"
  },
  "bugs": {
    "url": "https://github.com/Faba-network/fabalous-runtime-web/issues"
  },
  "homepage": "https://github.com/Faba-network/fabalous-runtime-web",
  "description": "",
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.*\\Spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
