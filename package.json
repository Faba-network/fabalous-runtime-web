{
  "name": "@fabalous/runtime-web",
  "version": "2.0.13",
  "scripts": {
    "postversion": "npm run build && git push && git push --tags && npm publish",
    "build": "gulp clean && tsc && gulp copy_src_to_lib && gulp remove_src_folder && gulp remove_node_modules_folder",
    "install": "shx cp -Rf ./lib/* ./ && shx rm -r ./lib",
    "test": "jest --no-cache",
    "tdd": "jest --watch",
    "coverage": "jest --no-cache --coverage",
    "doc": "gulp typedoc"
  },
  "author": "JÃ¶rg Wasmeier",
  "license": "ISC",
  "dependencies": {
    "sw-precache-webpack-plugin": "^0.11.5",
    "@fabalous/core": "^2.0.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "compression-webpack-plugin": "^1.1.11",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "offline-plugin": "^5.0.5",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^3.1.3",
    "shx": "^0.3.2",
    "ts-loader": "^4.5.0",
    "tslib": "^1.9.3",
    "typescript": "^3.0.1",
    "typestyle": "^2.0.1",
    "uglify-js": "^3.4.7",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-dev-server": "^3.1.5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-typedoc": "^2.2.0",
    "gulp-util": "^3.0.8",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "typedoc": "^0.12.0",
    "uglifyjs-webpack-plugin": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Faba-network/fabalous-runtime-web.git"
  },
  "bugs": {
    "url": "https://github.com/Faba-network/fabalous-runtime-web/issues"
  },
  "homepage": "https://github.com/Faba-network/fabalous-runtime-web",
  "description": "",
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.*\\Spec.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  }
}
